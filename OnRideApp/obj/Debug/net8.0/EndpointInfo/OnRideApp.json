{
  "openapi": "3.0.1",
  "info": {
    "title": "OnRideApp",
    "version": "1.0"
  },
  "paths": {
    "/api/Coupon": {
      "post": {
        "tags": [
          "Coupon"
        ],
        "parameters": [
          {
            "name": "coupon",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "discount",
            "in": "query",
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Customer": {
      "post": {
        "tags": [
          "Customer"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CustomerRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CustomerRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CustomerRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/gender/{gender}/age/{age}": {
      "get": {
        "tags": [
          "Customer"
        ],
        "parameters": [
          {
            "name": "gender",
            "in": "path",
            "required": true,
            "schema": {
              "$ref": "#/components/schemas/Gender"
            }
          },
          {
            "name": "age",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Driver": {
      "post": {
        "tags": [
          "Driver"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DriverRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/DriverRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/DriverRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Review/tripId/{tripId}": {
      "post": {
        "tags": [
          "Review"
        ],
        "parameters": [
          {
            "name": "tripId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ReviewRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/ReviewRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/ReviewRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Tracking": {
      "post": {
        "tags": [
          "Tracking"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LocationRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/LocationRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/LocationRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/tripId/{tripId}": {
      "get": {
        "tags": [
          "Tracking"
        ],
        "parameters": [
          {
            "name": "tripId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/TripBooking": {
      "post": {
        "tags": [
          "TripBooking"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/TripBookingRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/TripBookingRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/TripBookingRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CabRequest": {
        "type": "object",
        "properties": {
          "cabNumber": {
            "maxLength": 5,
            "type": "string",
            "nullable": true
          },
          "isAvailable": {
            "type": "boolean"
          },
          "cabSpecificationId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "CustomerRequest": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string"
          },
          "age": {
            "maximum": 110,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "emailId": {
            "type": "string",
            "format": "email",
            "nullable": true
          },
          "address": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "gender": {
            "$ref": "#/components/schemas/Gender"
          }
        },
        "additionalProperties": false
      },
      "DriverRequest": {
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 50,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "age": {
            "maximum": 60,
            "minimum": 18,
            "type": "integer",
            "format": "int32"
          },
          "panNumber": {
            "pattern": "^[A-Z]{5}[0-9]{4}[A-Z]{1}$",
            "type": "string",
            "nullable": true
          },
          "mobNumber": {
            "type": "string",
            "format": "tel",
            "nullable": true
          },
          "cab": {
            "$ref": "#/components/schemas/CabRequest"
          }
        },
        "additionalProperties": false
      },
      "Gender": {
        "enum": [
          0,
          1,
          2
        ],
        "type": "integer",
        "format": "int32"
      },
      "LocationRequest": {
        "type": "object",
        "properties": {
          "cabId": {
            "maximum": 2000,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "latitude": {
            "maximum": 90,
            "minimum": -90,
            "type": "number",
            "format": "double"
          },
          "longitude": {
            "maximum": 180,
            "minimum": -180,
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "ReviewRequest": {
        "type": "object",
        "properties": {
          "rating": {
            "maximum": 5,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "comment": {
            "maxLength": 500,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TripBookingRequest": {
        "type": "object",
        "properties": {
          "pickUp": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "destination": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "tripDistanceInKm": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "bookedAt": {
            "type": "string",
            "format": "date-time"
          },
          "customerEmailId": {
            "type": "string",
            "format": "email",
            "nullable": true
          },
          "cabSpecificationId": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      }
    }
  }
}